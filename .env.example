# App Configuration
NEXT_PUBLIC_APP_NAME=Okie
NEXT_PUBLIC_APP_DOMAIN=https://okie.one

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE=your_supabase_service_role_key

# CSRF Protection (required)
# Generate with: openssl rand -base64 32 | tr -dc 'A-Za-z0-9' | head -c32
CSRF_SECRET=your_32_character_random_string

# AI Model API Keys
OPENAI_API_KEY=sk-your_openai_api_key
MISTRAL_API_KEY=your_mistral_api_key
GOOGLE_GENERATIVE_AI_API_KEY=your_gemini_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
XAI_API_KEY=your_xai_api_key
PERPLEXITY_API_KEY=your_perplexity_api_key
OPENROUTER_API_KEY=your_openrouter_api_key

# Ollama Configuration (for local AI models)
OLLAMA_BASE_URL=http://localhost:11434
DISABLE_OLLAMA=true

# Required for BYOK functionality
# Generate with: openssl rand -base64 32
ENCRYPTION_KEY=

# Developer Tools (optional)
EXA_API_KEY=your_exa_api_key
GITHUB_TOKEN=your_github_token

# Production Configuration (optional)
# These are Vercel's automatic environment variables that contain your canonical production domain (without protocol)
# Used for consistent OAuth redirects - ensures redirects always point to production domain regardless of preview deployments
# NEXT_PUBLIC_VERCEL_PROJECT_PRODUCTION_URL is available client-side
# VERCEL_PROJECT_PRODUCTION_URL is available server-side only
# If you're not using Vercel, set these to your production domain
NEXT_PUBLIC_VERCEL_PROJECT_PRODUCTION_URL=your-production-domain.com
VERCEL_PROJECT_PRODUCTION_URL=your-production-domain.com

# Development Tools (optional)
ANALYZE=false
